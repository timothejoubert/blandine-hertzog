{
  "format": "custom",
  "id": "settings",
  "label": "settings",
  "repeatable": false,
  "status": true,
  "json": {
    "Main": {
      "site_name": {
        "type": "Text",
        "config": {
          "label": "Site name",
          "placeholder": "Mon site"
        }
      },
      "email": {
        "type": "Text",
        "config": {
          "label": "Email",
          "placeholder": ""
        }
      },
      "socials": {
        "type": "Group",
        "config": {
          "label": "Socials",
          "repeat": true,
          "fields": {
            "label": {
              "type": "Text",
              "config": {
                "label": "Label",
                "placeholder": ""
              }
            },
            "external_url": {
              "type": "Text",
              "config": {
                "label": "External url",
                "placeholder": ""
              }
            },
            "icon": {
              "type": "Select",
              "config": {
                "label": "Icon",
                "placeholder": "",
                "options": [
                  "instagram",
                  "facebook",
                  "x",
                  "tiktok",
                  "vimeo",
                  "behance",
                  "youtube",
                  "linkedin"
                ]
              }
            },
            "show_in_contact_slice": {
              "type": "Boolean",
              "config": {
                "label": "Show in contact slice",
                "placeholder_false": "false",
                "placeholder_true": "true",
                "default_value": false
              }
            }
          }
        }
      },
      "logo": {
        "type": "Image",
        "config": {
          "label": "logo",
          "constraint": {},
          "thumbnails": []
        }
      },
      "showreel": {
        "type": "Link",
        "config": {
          "label": "showreel",
          "placeholder": "",
          "select": "media",
          "allowText": true
        }
      }
    }
  }
}
